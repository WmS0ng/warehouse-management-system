<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.warehouse.mapper.PurchaseMapper">
    <!-- 添加采购单 -->
    <insert id="insertPurchase">
        insert into buy_list
        values (null, #{productId}, #{storeId}, #{buyNum}, #{factBuyNum}, now(), #{supplyId}, #{placeId}, #{buyUser},
                #{phone}, 0)
    </insert>
</mapper>
