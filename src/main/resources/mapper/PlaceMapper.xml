<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.warehouse.mapper.PlaceMapper">
    <!-- 查询所有产地 -->
    <select id="selectAllPlace" resultType="com.example.warehouse.entity.Place">
        select *
        from place
        where is_delete = 0
    </select>
</mapper>
